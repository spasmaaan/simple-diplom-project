{
  "name": "simple-diplom-project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "20.17.0",
    "pnpm": "9.11.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "preinstall": "npm install -g pnpm@9.11.0 & npx only-allow pnpm",
    "start": "webpack serve --env port=3001",
    "build:prod": "webpack --env mode=production",
    "build:dev": "webpack --env mode=development",
    "lint": "pnpm run lint:ts && pnpm run lint:styles && pnpm run prettier",
    "lint:fix": "pnpm run lint:ts:fix && pnpm run lint:styles:fix && pnpm run prettier:fix",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:styles": "stylelint --config .stylelintrc --ignore-path .stylelintignore \"**/*.{css,scss}\"",
    "lint:styles:fix": "stylelint --config .stylelintrc --ignore-path .stylelintignore \"**/*.{css,scss}\" --fix",
    "prettier": "prettier --check \"**/*.{ts,tsx,js,jsx,css,scss,json}\"",
    "prettier:fix": "prettier --write \"**/*.{ts,tsx,js,jsx,css,scss,json}\""
  },
  "license": "ISC",
  "private": true,
  "dependencies": {
    "@ant-design/charts": "^2.2.1",
    "@ant-design/icons": "^5.5.1",
    "antd": "^5.18.1",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@types/dotenv-webpack": "^7.0.7",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "browserslist": "^4.23.1",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.2.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "html-webpack-plugin": "^5.6.0",
    "lint-staged": "^15.2.5",
    "mini-css-extract-plugin": "^2.9.0",
    "prettier": "^3.3.1",
    "react-refresh": "^0.14.2",
    "sass": "^1.77.4",
    "sass-loader": "^14.2.1",
    "style-loader": "^4.0.0",
    "stylelint": "^16.6.1",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-prettier": "^5.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}
